@model List<Employee>;

@{
  ViewData["Title"] = "Employee";
}


<h1>This is employee page</h1>
<table class="table my-5">
  <thead>
    <tr>
        <th scope="col">Id</th>
      <th scope="col">FirstName</th>
      <th scope="col">LastName</th>
      <th scope="col">Address</th>
      <th scope="col">Age</th>
      <th scope="col">Edit</th>
      <th scope="col">Delete</th>
      
    </tr>
  </thead>
  <tbody>
      @foreach (var data in Model)
    {
                  <tr scope="row">
                  <td>@data.Id</td>
                  <td><a href="/employee/detail?id=@data.Id">@data.FirstName</a></td>
                  <td>@data.LastName</td>
                  <td>@data.Address</td>
                  <td>@data.Age</td>
                  <td><a href="/employee/edit?id=@data.Id"> <button type="button" class="btn btn-success">Edit</button></a></td>
                    <td><a href="/employee/delete?id=@data.Id"><button type="button" class="btn btn-danger">Delete</button></a> </td>


                  </tr>
    }


  </tbody>
</table>